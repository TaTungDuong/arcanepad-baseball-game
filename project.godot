[application]

config/name="Arcanepad Baseball Game"
run/main_scene="res://Scenes/Main/Main.tscn"
boot_splash/image="res://assets/sprites/objects/hust_boot_splash.png"
boot_splash/bg_color=Color( 0, 0, 0, 0 )
config/icon="res://assets/sprites/objects/hust_boot_splash.png"
config/windows_native_icon="res://assets/sprites/icons/hust.ico"

[autoload]

Arcane="*res://Arcanepad_Library/Arcane.tscn"
PlayerStatus="*res://Scenes/View/Player/PlayerStatus.tscn"
Database="*res://Scenes/View/Database/Database.tscn"

[display]

window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor]

version_control_autoload_on_startup=true
version_control_plugin_name="GitAPI"

[editor_plugins]

enabled=PoolStringArray( "res://addons/godot-sqlite/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0, 0, 0, 1 )
environment/default_environment="res://default_env.tres"
